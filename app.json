{
  "expo": {
    "name": "Windguru Spots",
    "slug": "windguru-spots-app",
    "version": "1.4.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gbe.windguruspots",
      "buildNumber": "1",
      "icon": "./assets/images/icon.png",
      "infoPlist": {
        "CFBundleIconName": "AppIcon",
        "UILaunchStoryboardName": "SplashScreen",
        "NSLocationWhenInUseUsageDescription": "We need your location to find wind spots near you",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to find wind spots near you"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gbe.windguruspots"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "description": "An app to check your preferred sailing and surf spots using Windguru forecasts",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "f60a1e17-5377-4b43-b2de-0b42c5aa0c42"
      }
    },
    "owner": "gonzalobandeira",
    "supportUrl": "https://gonzalobandeira.github.io/windguru-spots/docs/SUPPORT.html",
    "privacyUrl": "https://gonzalobandeira.github.io/windguru-spots/docs/PRIVACY.html"
  }
}
