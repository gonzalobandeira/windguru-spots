{
  "name": "Test Release Workflow",
  "description": "Test workflow that simulates a release build for the current branch",
  "trigger": {
    "source": "github",
    "event": "push",
    "branches": ["ban-61-setup-xcode-cloud-for-automatic-releasesuploads"]
  },
  "actions": {
    "build": {
      "scheme": "WindguruSpots",
      "platform": "iOS",
      "configuration": "Release",
      "destination": "appstore",
      "environment": {
        "CI": "true",
        "EXPO_USE_COMMUNITY_AUTOLINKING": "1",
        "EXPO_DEV_CLIENT_NETWORK_INSPECTOR": "true",
        "NODE_ENV": "production"
      }
    }
  },
  "environment": {
    "xcode": {
      "version": "latest"
    },
    "macOS": {
      "version": "latest"
    }
  }
}

